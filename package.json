{
  "name": "url-to-s3",
  "version": "0.1.0",
  "description": "GitHub Action to fetch content from any URL and upload to S3",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/predictr-io/url_to_s3.git"
  },
  "keywords": [
    "github-actions",
    "s3",
    "aws",
    "download",
    "upload"
  ],
  "author": "predictr-io",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@aws-sdk/client-s3": "^3.679.0",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
